      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="pulseid" UNIQUE="false" FIELDS="pulseid, userid"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
