<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/pulse/addons/credits/db" VERSION="20211009" COMMENT="XMLDB file for Pulse Reaction addon"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="pulseaddon_credits" COMMENT="pulseaddon_credits table retrofitted from MySQL">
